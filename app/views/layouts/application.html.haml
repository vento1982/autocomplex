!!!
%head
	%title Autocomplex
	= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track' => true
	= stylesheet_link_tag 'application', 'https://fonts.googleapis.com/css?family=Roboto', media: 'all', 'data-turbolinks-track' => true
	= javascript_include_tag 'application', 'data-turbolinks-track' => true
	= csrf_meta_tags
%body
	= nav_bar static: :top, brand: image_tag("ac.png"), responsive: true do
		= menu_group do
			= menu_item 'About Us', about_path
			= menu_item 'Contact', contact_path
		- if current_user
			= menu_group pull: :right do
				= menu_item 'Logout', destroy_user_session_path, method: :delete
	.jumbotron
		%h1 Test
		%p.notice= notice
		%p.alert= alert
	.container-fluid
		= yield